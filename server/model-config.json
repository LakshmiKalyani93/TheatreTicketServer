{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "Ticket": {
    "dataSource": "mongodb",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "create": true,
          "deleteById": true,
          "findById": true,
          "count": true,
          "prototype.patchAttributes": true,
          "find": true,
          "getEarnedAnalytics": true,
          "getVisitedAnalytics":true
        }
      }
    }
  }
}